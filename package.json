{
  "name": "dev-enigma",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://carefree-ladka.in",
  "description": "A developer blog and documentation site built with Docusaurus",
  "author": "Pawan Kumar",
  "license": "MIT",
  "scripts": {
    "test": "echo \"No tests configured\" && exit 0",
    "docusaurus": "docusaurus",
    "dev": "docusaurus start",
    "start": "npm run dev",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.md,.mdx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.md,.mdx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yml,yaml}\"",
    "validate": "npm run format:check && npm run lint && npm run typecheck",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:force": "gh-pages -d build -f"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@docusaurus/theme-mermaid": "3.5.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "prismjs": "^1.30.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rough-notation": "^0.5.1"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "3.9.1",
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/tsconfig": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "15.5.2",
    "prettier": "3.6.2",
    "typescript": "~5.5.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ],
    "*.{json,css,scss,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "docusaurus",
    "blog",
    "documentation",
    "developer",
    "portfolio",
    "interview experiences"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carefree-ladka/dev.enigma.git"
  },
  "bugs": {
    "url": "https://github.com/carefree-ladka/dev.enigma/issues"
  }
}
